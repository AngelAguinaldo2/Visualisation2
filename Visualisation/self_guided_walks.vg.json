{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 800,
    "height": 450,
    "title": "Self Guided Walks in Melbourne",
    "layer":[

        {
            "data": {
                "url":"https://raw.githubusercontent.com/AngelAguinaldo2/Visualisation2/main/Maps/road-corridors.topojson",
                "format": {"type": "topojson", "feature": "road-corridors"}

            },
            "mark": {"type": "geoshape", "fill": "null", "stroke":"grey", "opacity":0.5}

        },
        

        {
            "data": {
                "url":"https://raw.githubusercontent.com/AngelAguinaldo2/Visualisation2/main/Maps/self-guided-walks.topojson",

                "format": {"type": "topojson", "feature": "self-guided-walks"}
            },
            "mark": {"type": "geoshape", "fillOpacity":0.75, "stroke":"black", "strokeWidth":1},
            "encoding": {
                "color": {"field": "properties.name", "type": "nominal"}
              }
        },

        {
            "data": {
                "url":"https://raw.githubusercontent.com/AngelAguinaldo2/Visualisation2/main/Maps/street-names.json",
                "format": {"type": "topojson", "feature": "street-names"}

            },
            "mark": {"type": "text"},
            "encoding": {"text":{"field":"properties.maplabel", "type": "nominal"}}

        }

        
    ]
}